<div class="page-header">
  <h1>
    Red Flags

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th></th>
        <th>Deputy Name</th>
        <th>Expense description</th>
        <th>Expense detail</th>
        <th>Expense amount</th>
        <th>Actions</th>
      </tr>

      <% @red_flag_expense_deputies.each do |red_flag_expense_deputy| %>
      <tr>
        <td><%= red_flag_expense_deputy.user_id %></td>
        <td><%= red_flag_expense_deputy.deputy_id %></td>
        <td><%= red_flag_expense_deputy.expense_description %></td>
        <td><%= red_flag_expense_deputy.expense_detail %></td>
        <td><%= red_flag_expense_deputy.expense_amount %></td>
        <td>
          <a href="/red_flag_expense_deputies/<%= red_flag_expense_deputy.id %>" class="btn btn-primary">Show</a>
          <a href="/red_flag_expense_deputies/<%= red_flag_expense_deputy.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_red_flag_expense_deputy/<%= red_flag_expense_deputy.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
