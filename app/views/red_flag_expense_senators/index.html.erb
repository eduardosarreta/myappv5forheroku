<div class="page-header">
  <h1>
    Red flag expense senators
    <a href="/red_flag_expense_senators/new" class="btn btn-lg btn-success">New Red flag expense senator</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Senator</th>
        <th>Expense description</th>
        <th>Expense detail</th>
        <th>Expense amount</th>
        <th>Actions</th>
      </tr>

      <% @red_flag_expense_senators.each do |red_flag_expense_senator| %>
      <tr>
        <td><%= red_flag_expense_senator.user_id %></td>
        <td><%= red_flag_expense_senator.senator_id %></td>
        <td><%= red_flag_expense_senator.expense_description %></td>
        <td><%= red_flag_expense_senator.expense_detail %></td>
        <td><%= red_flag_expense_senator.expense_amount %></td>
        <td>
          <a href="/red_flag_expense_senators/<%= red_flag_expense_senator.id %>" class="btn btn-primary">Show</a>
          <a href="/red_flag_expense_senators/<%= red_flag_expense_senator.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_red_flag_expense_senator/<%= red_flag_expense_senator.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
