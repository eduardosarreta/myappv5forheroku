<div class="page-header">
  <h3>Bookmarked Senator #<%= @bookmarked_senator.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @bookmarked_senator.user_id %></dd>

      <dt>Senator</dt>
      <dd><%= @bookmarked_senator.senator_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookmarked_senators" class="btn btn-primary">
        Back
      </a>
      <a href="/bookmarked_senators/<%= @bookmarked_senator.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_bookmarked_senator/<%= @bookmarked_senator.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
