<div class="page-header">
  <h1>
    Comments expense senators
    <a href="/comments_expense_senators/new" class="btn btn-lg btn-success">New Comments expense senator</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Body</th>
        <th>Red flag expense senator</th>
        <th>Senator</th>
        <th>Actions</th>
      </tr>

      <% @comments_expense_senators.each do |comments_expense_senator| %>
      <tr>
        <td><%= comments_expense_senator.user_id %></td>
        <td><%= comments_expense_senator.body %></td>
        <td><%= comments_expense_senator.red_flag_expense_senator_id %></td>
        <td><%= comments_expense_senator.senator_id %></td>
        <td>
          <a href="/comments_expense_senators/<%= comments_expense_senator.id %>" class="btn btn-primary">Show</a>
          <a href="/comments_expense_senators/<%= comments_expense_senator.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_comments_expense_senator/<%= comments_expense_senator.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
