<div class="page-header">
  <h3>Bookmarked Deputy #<%= @bookmarked_deputy.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @bookmarked_deputy.user_id %></dd>

      <dt>Deputy</dt>
      <dd><%= @bookmarked_deputy.deputy_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookmarked_deputies" class="btn btn-primary">
        Back
      </a>
      <a href="/bookmarked_deputies/<%= @bookmarked_deputy.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_bookmarked_deputy/<%= @bookmarked_deputy.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
